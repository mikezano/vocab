@page "/translations"
@using Vocab.Components
@using Vocab.State
@inject AppState AppState
@inject NavigationManager NavigationManager


@if (AppState.SheetId != null)
{
    <Cards />
}

@code {
    protected override void OnInitialized()
    {
       if(AppState.SheetId == null)
       {
           NavigationManager.NavigateTo("/");
       }
    }
}
