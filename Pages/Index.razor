@page "/";
@using Vocab.Components
@using Vocab.Models
@using Vocab.State
@inject IJSRuntime JS
@inject AppState AppState

@* <Navigation></Navigation> *@


<h1 class="text-center  p-0 m-0 vocab-title">Vocab</h1>
<div>SheetId: @AppState.SheetId</div>

    @if (AppState.SheetId == null)
    {
        <GoogleSheetForm />

    }
    else
    {
        <Cards />
        <div>Other stuff</div>

    }



@code{
    private bool _isInitialized = true;
    protected override async Task OnInitializedAsync()
    {
        //AppState.OnSetSheetId += HandleSetSheetId;
        await AppState.InitializeAsync();
        _isInitialized = true;

    }   
}